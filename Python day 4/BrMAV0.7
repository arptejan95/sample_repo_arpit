CREATE OR REPLACE PROCEDURE DISHA_MART_ANALYTICS.FINANCE_MART.LOAD_BRANCH_MONTHLY_ACTIVITY_MART()
RETURNS VARCHAR
LANGUAGE SQL
EXECUTE AS CALLER
AS '
BEGIN

    -- STEP 1: INSERT DATA INTO BRANCH_MONTHLY_ACTIVITY_MART
	CREATE OR REPLACE TABLE DISHA_MART_ANALYTICS.FINANCE_MART.BRANCH_MONTHLY_ACTIVITY_MART (
	PCMCS_BRANCH_ID VARCHAR(20),
	LMS_BRANCH_ID VARCHAR(20),
	BRANCH_NAME VARCHAR(225),
	BRANCH_OPENING_DATE DATE,
	BRANCH_VINTAGE VARCHAR(225),
	STATE_NAME VARCHAR(225),
	MONTH_YEAR VARCHAR(10),
	MONTH_END_DATE DATE,
	
	BRANCH_DIRECT_INCOME_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_INTEREST_INCOME_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_OTHER_INCOME_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_EXPENDITURE_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_OTHER_OPERATING_EXPENSES_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_PROVISION_FOR_DELIQUENCIES_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_PBT_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_DEPRECIATION_AND_TAX_EXPENSE NUMBER(38,5),
	BRANCH_DIRECT_PAT_AFTER_ADJ NUMBER(38,5),
	
	ALLOCATED_INCOME_FROM_HO NUMBER(38,5),
	ALLOCATED_INCOME_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_INTEREST_INCOME_FROM_HO NUMBER(38,5),
	ALLOCATED_INTEREST_INCOME_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_OTHER_INCOME_FROM_HO NUMBER(38,5),
	ALLOCATED_OTHER_INCOME_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_EXPENDITURE_FROM_HO NUMBER(38,5),
	ALLOCATED_EXPENDITURE_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_EMPLOYEE_BENEFIT_EXPENSE_FROM_HO NUMBER(38,5),
	ALLOCATED_EMPLOYEE_BENEFIT_EXPENSE_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_OTHER_OPERATING_EXPENSES_FROM_HO NUMBER(38,5),
	ALLOCATED_OTHER_OPERATING_EXPENSES_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_PROVISION_FOR_DELIQUENCIES_FROM_HO NUMBER(38,5),
	ALLOCATED_PROVISION_FOR_DELIQUENCIES_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_PBT_FROM_HO NUMBER(38,5),
	ALLOCATED_PBT_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_DEPRECIATION_AND_TAX_EXPENSE_FROM_HO NUMBER(38,5),
	ALLOCATED_DEPRECIATION_AND_TAX_EXPENSE_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_PAT_FROM_HO NUMBER(38,5),
	ALLOCATED_PAT_FROM_BRANCH NUMBER(38,5),
	
	BRANCH_TOTAL_INCOME_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_INTEREST_INCOME_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_OTHER_INCOME_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_EXPENDITURE_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_OTHER_OPERATING_EXPENSES_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_PROVISION_FOR_DELIQUENCIES_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_PBT_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_DEPRECIATION_AND_TAX_EXPENSE_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_PAT_AFTER_ALLOCATION NUMBER(38,5),
	
	HL_AUM NUMBER(38,5),
	NHL_AUM NUMBER(38,5),
	NET_AUM NUMBER(38,5),
	ROA NUMBER(38,2),
	NET_DISBURSAL_AMOUNT NUMBER(38,5),
	DISBURSAL_YIELD NUMBER(38,2),
	DISBURSAL_TARGET NUMBER(38,2),
	NET_SANCTION_COUNT NUMBER(38,0),
	NET_SANCTION_AMOUNT NUMBER(38,5),
	NET_SUD_COUNT NUMBER(38,0),
	NET_SUD_AMOUNT NUMBER(38,5),
	NET_LOGIN_COUNT NUMBER(38,0),
	NET_LOGIN_AMOUNT NUMBER(38,5),
	NET_PROCESSING_FEE NUMBER(38,5),
	NET_PDD_FEE NUMBER(38,5),
	NET_ADMIN_FEE NUMBER(38,5),
	NET_TOTAL_FEES_AMOUNT NUMBER(38,5),
	NET_PROPERTY_INSURANCE_FEE NUMBER(38,5),
	NET_LIFE_INSURANCE_FEE NUMBER(38,5),
	NET_HEALTH_INSURANCE_FEE NUMBER(38,5),
 	SALES_RO_HEADCOUNT NUMBER(38,0),
	SALES_RRO_HEADCOUNT NUMBER(38,0),
	SALES_BH_HEADCOUNT NUMBER(38,0),
	CREDIT_CO_HEADCOUNT NUMBER(38,0),
	CREDIT_CM_HEADCOUNT NUMBER(38,0),
	BT_IN_CASES NUMBER(38,0),
	BT_IN_AMOUNT NUMBER(38,5),
	BT_OUT_CASES NUMBER(38,0),
	BT_OUT_AMOUNT NUMBER(38,5),
	HL_BOUNCE_COUNT NUMBER(38,0),
	NHL_BOUNCE_COUNT NUMBER(38,0),
	HL_30PLUS_COUNT NUMBER(38,0),
	NHL_30PLUS_COUNT NUMBER(38,0),
	HL_60PLUS_COUNT NUMBER(38,0),
	NHL_60PLUS_COUNT NUMBER(38,0),
	HL_90PLUS_COUNT NUMBER(38,0),
	NHL_90PLUS_COUNT NUMBER(38,0),
	MART_PROCESSING_DATE DATE
	);


    -- STEP 2: INSERT DATA INTO BRANCH_MONTHLY_ACTIVITY_PREFINAL
	CREATE OR REPLACE TEMPORARY TABLE DISHA_MART_ANALYTICS.FINANCE_MART.BRANCH_MONTHLY_ACTIVITY_PREFINAL
	(
	PCMCS_BRANCH_ID VARCHAR(20),
	LMS_BRANCH_ID VARCHAR(20),
	BRANCH_NAME VARCHAR(225),
	BRANCH_OPENING_DATE DATE,
	BRANCH_VINTAGE VARCHAR(225),
	STATE_NAME VARCHAR(225),
	MONTH_YEAR VARCHAR(10),
	MONTH_END_DATE DATE,
	
	BRANCH_DIRECT_INCOME_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_INTEREST_INCOME_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_OTHER_INCOME_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_EXPENDITURE_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_OTHER_OPERATING_EXPENSES_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_PROVISION_FOR_DELIQUENCIES_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_PBT_AFTER_ADJ NUMBER(38,5),
	BRANCH_DIRECT_DEPRECIATION_AND_TAX_EXPENSE NUMBER(38,5),
	BRANCH_DIRECT_PAT_AFTER_ADJ NUMBER(38,5),
	
	ALLOCATED_INCOME_FROM_HO NUMBER(38,5),
	ALLOCATED_INCOME_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_INTEREST_INCOME_FROM_HO NUMBER(38,5),
	ALLOCATED_INTEREST_INCOME_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_OTHER_INCOME_FROM_HO NUMBER(38,5),
	ALLOCATED_OTHER_INCOME_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_EXPENDITURE_FROM_HO NUMBER(38,5),
	ALLOCATED_EXPENDITURE_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_EMPLOYEE_BENEFIT_EXPENSE_FROM_HO NUMBER(38,5),
	ALLOCATED_EMPLOYEE_BENEFIT_EXPENSE_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_OTHER_OPERATING_EXPENSES_FROM_HO NUMBER(38,5),
	ALLOCATED_OTHER_OPERATING_EXPENSES_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_PROVISION_FOR_DELIQUENCIES_FROM_HO NUMBER(38,5),
	ALLOCATED_PROVISION_FOR_DELIQUENCIES_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_PBT_FROM_HO NUMBER(38,5),
	ALLOCATED_PBT_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_DEPRECIATION_AND_TAX_EXPENSE_FROM_HO NUMBER(38,5),
	ALLOCATED_DEPRECIATION_AND_TAX_EXPENSE_FROM_BRANCH NUMBER(38,5),
	ALLOCATED_PAT_FROM_HO NUMBER(38,5),
	ALLOCATED_PAT_FROM_BRANCH NUMBER(38,5),
	
	BRANCH_TOTAL_INCOME_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_INTEREST_INCOME_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_OTHER_INCOME_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_EXPENDITURE_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_OTHER_OPERATING_EXPENSES_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_PROVISION_FOR_DELIQUENCIES_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_PBT_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_DEPRECIATION_AND_TAX_EXPENSE_AFTER_ALLOCATION NUMBER(38,5),
	BRANCH_TOTAL_PAT_AFTER_ALLOCATION NUMBER(38,5),
	
	HL_AUM NUMBER(38,5),
	NHL_AUM NUMBER(38,5),
	NET_AUM NUMBER(38,5),
	ROA NUMBER(38,2),
	NET_DISBURSAL_AMOUNT NUMBER(38,5),
	DISBURSAL_YIELD NUMBER(38,2),
	DISBURSAL_TARGET NUMBER(38,2),
	NET_SANCTION_COUNT NUMBER(38,0),
	NET_SANCTION_AMOUNT NUMBER(38,5),
	NET_SUD_COUNT NUMBER(38,0),
	NET_SUD_AMOUNT NUMBER(38,5),
	NET_LOGIN_COUNT NUMBER(38,0),
	NET_LOGIN_AMOUNT NUMBER(38,5),
	NET_PROCESSING_FEE NUMBER(38,5),
	NET_PDD_FEE NUMBER(38,5),
	NET_ADMIN_FEE NUMBER(38,5),
	NET_TOTAL_FEES_AMOUNT NUMBER(38,5),
	NET_PROPERTY_INSURANCE_FEE NUMBER(38,5),
	NET_LIFE_INSURANCE_FEE NUMBER(38,5),
	NET_HEALTH_INSURANCE_FEE NUMBER(38,5),
	SALES_RO_HEADCOUNT NUMBER(38,0),
	SALES_RRO_HEADCOUNT NUMBER(38,0),
	SALES_BH_HEADCOUNT NUMBER(38,0),
	CREDIT_CO_HEADCOUNT NUMBER(38,0),
	CREDIT_CM_HEADCOUNT NUMBER(38,0),
	BT_IN_CASES NUMBER(38,0),
	BT_IN_AMOUNT NUMBER(38,5),
	BT_OUT_CASES NUMBER(38,0),
	BT_OUT_AMOUNT NUMBER(38,5),
	HL_BOUNCE_COUNT NUMBER(38,0),
	NHL_BOUNCE_COUNT NUMBER(38,0),
	HL_30PLUS_COUNT NUMBER(38,0),
	NHL_30PLUS_COUNT NUMBER(38,0),
	HL_60PLUS_COUNT NUMBER(38,0),
	NHL_60PLUS_COUNT NUMBER(38,0),
	HL_90PLUS_COUNT NUMBER(38,0),
	NHL_90PLUS_COUNT NUMBER(38,0),
    MART_PROCESSING_DATE DATE
    ) 
	AS
	(
    SELECT
		BID.PCMCS_BRANCH_ID,
		BID.LMS_BRANCH_ID,
		BID.BRANCH_NAME,
		BID.BRANCH_OPENING_DATE,
		CONCAT(
			FLOOR(TIMESTAMPDIFF(MONTH, BID.BRANCH_OPENING_DATE, BID.MONTH_END_DATE) / 12), '' Year, '',
			MOD(TIMESTAMPDIFF(MONTH, BID.BRANCH_OPENING_DATE, BID.MONTH_END_DATE), 12), '' Months''
		) AS BRANCH_VINTAGE,
		BID.STATE_NAME,
		BID.MONTH_YEAR,
		BID.MONTH_END_DATE,
		BID.BRANCH_DIRECT_INCOME_AFTER_ADJ,
		BID.BRANCH_DIRECT_INTEREST_INCOME_AFTER_ADJ,
		BID.BRANCH_DIRECT_OTHER_INCOME_AFTER_ADJ,
		BID.BRANCH_DIRECT_EXPENDITURE_AFTER_ADJ,
		BID.BRANCH_DIRECT_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ADJ,
		BID.BRANCH_DIRECT_OTHER_OPERATING_EXPENSES_AFTER_ADJ,
		BID.BRANCH_DIRECT_PROVISION_FOR_DELIQUENCIES_AFTER_ADJ,
		BID.BRANCH_DIRECT_PBT_AFTER_ADJ,
		BID.BRANCH_DIRECT_DEPRECIATION_AND_TAX_EXPENSE,
		BID.BRANCH_DIRECT_PAT_AFTER_ADJ,
		
		BID.ALLOCATED_INCOME_FROM_HO,
		BID.ALLOCATED_INCOME_FROM_BRANCH,
		BID.ALLOCATED_INTEREST_INCOME_FROM_HO,
		BID.ALLOCATED_INTEREST_INCOME_FROM_BRANCH,
		BID.ALLOCATED_OTHER_INCOME_FROM_HO,
		BID.ALLOCATED_OTHER_INCOME_FROM_BRANCH,
		BID.ALLOCATED_EXPENDITURE_FROM_HO,
		BID.ALLOCATED_EXPENDITURE_FROM_BRANCH,
		BID.ALLOCATED_EMPLOYEE_BENEFIT_EXPENSE_FROM_HO,
		BID.ALLOCATED_EMPLOYEE_BENEFIT_EXPENSE_FROM_BRANCH,
		BID.ALLOCATED_OTHER_OPERATING_EXPENSES_FROM_HO,
		BID.ALLOCATED_OTHER_OPERATING_EXPENSES_FROM_BRANCH,
		BID.ALLOCATED_PROVISION_FOR_DELIQUENCIES_FROM_HO,
		BID.ALLOCATED_PROVISION_FOR_DELIQUENCIES_FROM_BRANCH,
		BID.ALLOCATED_PBT_FROM_HO,
		BID.ALLOCATED_PBT_FROM_BRANCH,
		BID.ALLOCATED_DEPRECIATION_AND_TAX_EXPENSE_FROM_HO,
		BID.ALLOCATED_DEPRECIATION_AND_TAX_EXPENSE_FROM_BRANCH,
		BID.ALLOCATED_PAT_FROM_HO,
		BID.ALLOCATED_PAT_FROM_BRANCH,
		
		BID.BRANCH_TOTAL_INCOME_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_INTEREST_INCOME_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_OTHER_INCOME_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_EXPENDITURE_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_OTHER_OPERATING_EXPENSES_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_PROVISION_FOR_DELIQUENCIES_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_PBT_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_DEPRECIATION_AND_TAX_EXPENSE_AFTER_ALLOCATION,
		BID.BRANCH_TOTAL_PAT_AFTER_ALLOCATION,
		
        COALESCE(RLM.HL_AUM,0) AS HL_AUM,
        COALESCE(RLM.NHL_AUM,0) AS NHL_AUM,
		COALESCE(RLM.NET_AUM,0) AS NET_AUM,
		COALESCE(BID.BRANCH_TOTAL_PAT_AFTER_ALLOCATION/NULLIF(RLM.NET_AUM,0),0) AS ROA,
		COALESCE(LAM_DISB.NET_DISBURSAL_AMOUNT,0) AS NET_DISBURSAL_AMOUNT,
		COALESCE(DISB_YLD.DISBURSAL_YIELD,0) AS DISBURSAL_YIELD,
		DISB_TGT.DISBURSAL_TARGET,
		COALESCE(LAM_SANC.NET_SANCTION_COUNT,0) AS NET_SANCTION_COUNT,
		COALESCE(LAM_SANC.NET_SANCTION_AMOUNT,0) AS NET_SANCTION_AMOUNT,
		COALESCE(LAM_SANC.NET_SUD_COUNT,0) AS NET_SUD_COUNT,
		COALESCE(LAM_SANC.NET_SUD_AMOUNT,0) AS NET_SUD_AMOUNT,
		COALESCE(LAM_DEAL.NET_LOGIN_COUNT,0) AS NET_LOGIN_COUNT,
		COALESCE(LAM_DEAL.NET_LOGIN_AMOUNT,0) AS NET_LOGIN_AMOUNT,
		COALESCE(LACCM_FEE.NET_PROCESSING_FEE,0) AS NET_PROCESSING_FEE,
		COALESCE(LACCM_FEE.NET_PDD_FEE,0) AS NET_PDD_FEE,
		COALESCE(LACCM_FEE.NET_ADMIN_FEE,0) AS NET_ADMIN_FEE,
		COALESCE(LACCM_FEE.NET_TOTAL_FEES_AMOUNT,0) AS NET_TOTAL_FEES_AMOUNT,
		COALESCE(LACCM_FEE.NET_PROPERTY_INSURANCE_FEE,0) AS NET_PROPERTY_INSURANCE_FEE,
		COALESCE(LACCM_FEE.NET_LIFE_INSURANCE_FEE,0) AS NET_LIFE_INSURANCE_FEE,
		COALESCE(LACCM_FEE.NET_HEALTH_INSURANCE_FEE,0) AS NET_HEALTH_INSURANCE_FEE,
		EMP_CNT.SALES_RO_HEADCOUNT,
		EMP_CNT.SALES_RRO_HEADCOUNT,
		EMP_CNT.SALES_BH_HEADCOUNT,
		EMP_CNT.CREDIT_CO_HEADCOUNT,
		EMP_CNT.CREDIT_CM_HEADCOUNT,
        NULL AS BT_IN_CASES,
	    NULL AS BT_IN_AMOUNT,
	    NULL AS BT_OUT_CASES,
	    NULL AS BT_OUT_AMOUNT,
	    NULL AS HL_BOUNCE_COUNT,
	    NULL AS NHL_BOUNCE_COUNT,
	    NULL AS HL_30PLUS_COUNT,
	    NULL AS NHL_30PLUS_COUNT,
	    NULL AS HL_60PLUS_COUNT,
	    NULL AS NHL_60PLUS_COUNT,
	    NULL AS HL_90PLUS_COUNT,
	    NULL AS NHL_90PLUS_COUNT,
        CURRENT_DATE() AS MART_PROCESSING_DATE
		FROM
			(   
			SELECT
				BPD.BRANCH AS PCMCS_BRANCH_ID,
				BDSFDC.BRANCH_LMS_CODE AS LMS_BRANCH_ID,
				BDSFDC.BRANCH_NAME,
				BDSFDC.BRANCH_STATE_NAME AS STATE_NAME,
				BDSFDC.DATE_OF_OPENING AS BRANCH_OPENING_DATE,
				--BPD.PERIOD || SUBSTRING(BPD.YEAR, 3, 2) AS MONTH_YEAR,
				BPD.PERIOD || 
				CASE WHEN UPPER(BPD.PERIOD) IN (''JAN'', ''FEB'', ''MAR'') 
					THEN LPAD(CAST(SUBSTRING(BPD.YEAR, 3, 2) AS INT) + 1, 2, ''0'')
				ELSE 
					SUBSTRING(BPD.YEAR, 3, 2)
				END AS MONTH_YEAR,
				LAST_DAY(TO_DATE(CONCAT(''01-'', MONTH_YEAR), ''DD-MonYY'')) AS MONTH_END_DATE,
				
				-----------/* DIRECT INCOME/EXPENSE */------------
				
				
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_40000000'' 
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'')
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) = ''PCM_INPUT''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_DIRECT_INCOME_AFTER_ADJ,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_41010000'' 
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'')
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) = ''PCM_INPUT''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_DIRECT_INTEREST_INCOME_AFTER_ADJ,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) IN (''AC_41030000'', ''AC_41040000'', ''AC_42000000'')
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'')
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) = ''PCM_INPUT''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_DIRECT_OTHER_INCOME_AFTER_ADJ,
				
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_50000000''
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'')
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) = ''PCM_INPUT''
						THEN (AMOUNT) 
						ELSE 0 
					END) -
                SUM(CASE 
                        WHEN UPPER(TRIM(ACCOUNT)) IN (''AC_51070000'', ''AC_51090000'')
                            AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
                            AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
                            AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'')
                            AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
                            AND UPPER(TRIM(BALANCE)) = ''PCM_INPUT''
                        THEN (AMOUNT)
                        ELSE 0 
                    END) AS BRANCH_DIRECT_EXPENDITURE_AFTER_ADJ,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_51060000''
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'')
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) = ''PCM_INPUT''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_DIRECT_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ADJ,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) IN (''AC_51010000'', ''AC_51020000'', ''AC_51080000'')
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'')
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) = ''PCM_INPUT''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_DIRECT_OTHER_OPERATING_EXPENSES_AFTER_ADJ,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_51050000''
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
