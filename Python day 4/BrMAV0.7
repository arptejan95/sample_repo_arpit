							AND TRIM(RULE) IN (''S1_Interest_Cost'', ''S2_Interest_Cost'', ''S3_Interest_Cost'', ''S4_Interest_Cost'', ''S1_Periodic_Disbursement'', ''S1_90_Plus_AUM'', ''S2_90_Plus_AUM'', ''S3_90_Plus_AUM'', ''S4_90_Plus_AUM'', ''S1_90_Plus_AUM_HL'', ''S2_90_Plus_AUM_HL'', ''S3_90_Plus_AUM_HL'', ''S4_90_Plus_AUM_HL'', ''S1_90_Plus_AUM_NHL'', ''S2_90_Plus_AUM_NHL'', ''S3_90_Plus_AUM_NHL'', ''S4_90_Plus_AUM_NHL'', ''S1_Disbursements'', ''S2_Disbursements'', ''S3_Disbursements'', ''S4_Disbursements'', ''S1_AUM'', ''S2_AUM'', ''S3_AUM'', ''S4_AUM'', ''S1_AUM_HL'', ''S2_AUM_HL'', ''S3_AUM_HL'', ''S4_AUM_HL'', ''S1_AUM_NHL'', ''S2_AUM_NHL'', ''S3_AUM_NHL'', ''S4_AUM_NHL'', ''S1_PBT_of_Branch'', ''S2_PBT_of_Branch'', ''S3_PBT_of_Branch'', ''S4_PBT_of_Branch'', ''S2_Periodic_Disbursement'', ''S3_Periodic_Disbursement'', ''S4_Periodic_Disbursement'', ''S1_Headcount'', ''S2_Headcount'', ''S3_Headcount'', ''S4_Headcount'', ''S1_New_Business_YTD'', ''S2_New_Business_YTD'', ''S3_New_Business_YTD'', ''S4_New_Business_YTD'', ''S1_No_of_1_Plus_Cases'', ''S2_No_of_1_Plus_Cases'', ''S3_No_of_1_Plus_Cases'', ''S4_No_of_1_Plus_Cases'', ''S1_No_of_Cases_Disbursed'', ''S2_No_of_Cases_Disbursed'', ''S3_No_of_Cases_Disbursed'', ''S4_No_of_Cases_Disbursed'', ''S1_No_of_Live_Cases'', ''S2_No_of_Live_Cases'', ''S3_No_of_Live_Cases'', ''S4_No_of_Live_Cases'', ''S1_Assigned_Cases_AUM'', ''S2_Assigned_Cases_AUM'', ''S3_Assigned_Cases_AUM'', ''S4_Assigned_Cases_AUM'', ''S1_Interest_Rate'', ''S2_Interest_Rate'', ''S3_Interest_Rate'', ''S4_Interest_Rate'', ''S2_and_S3_No_of_Headcount_New_Business_YTD'', ''S1_No_of_Headcount'', ''S4_No_of_Headcount'', ''Copy_to_New_Business_YTD_from_Headcount_Net_Balance_HO'', /*Allocation In */
							''S1_Interest_Cost'', ''S2_Interest_Cost'', ''S3_Interest_Cost'', ''S4_Interest_Cost'', ''S1_Periodic_Disbursement'', ''S1_90_Plus_AUM'', ''S2_90_Plus_AUM'', ''S3_90_Plus_AUM'', ''S4_90_Plus_AUM'', ''S1_90_Plus_AUM_HL'', ''S2_90_Plus_AUM_HL'', ''S3_90_Plus_AUM_HL'', ''S4_90_Plus_AUM_HL'', ''S1_90_Plus_AUM_NHL'', ''S2_90_Plus_AUM_NHL'', ''S3_90_Plus_AUM_NHL'', ''S4_90_Plus_AUM_NHL'', ''S1_Disbursements'', ''S2_Disbursements'', ''S3_Disbursements'', ''S4_Disbursements'', ''S1_AUM'', ''S2_AUM'', ''S3_AUM'', ''S4_AUM'', ''S1_AUM_HL'', ''S2_AUM_HL'', ''S3_AUM_HL'', ''S4_AUM_HL'', ''S1_AUM_NHL'', ''S2_AUM_NHL'', ''S3_AUM_NHL'', ''S4_AUM_NHL'', ''S1_PBT_of_Branch'', ''S2_PBT_of_Branch'', ''S3_PBT_of_Branch'', ''S4_PBT_of_Branch'', ''S2_Periodic_Disbursement'', ''S3_Periodic_Disbursement'', ''S4_Periodic_Disbursement'', ''S1_Headcount'', ''S2_Headcount'', ''S3_Headcount'', ''S4_Headcount'', ''S1_New_Business_YTD'', ''S2_New_Business_YTD'', ''S3_New_Business_YTD'', ''S4_New_Business_YTD'', ''S1_No_of_1_Plus_Cases'', ''S2_No_of_1_Plus_Cases'', ''S3_No_of_1_Plus_Cases'', ''S4_No_of_1_Plus_Cases'', ''S1_No_of_Cases_Disbursed'', ''S2_No_of_Cases_Disbursed'', ''S3_No_of_Cases_Disbursed'', ''S4_No_of_Cases_Disbursed'', ''S1_No_of_Live_Cases'', ''S2_No_of_Live_Cases'', ''S3_No_of_Live_Cases'', ''S4_No_of_Live_Cases'', ''S1_Assigned_Cases_AUM'', ''S2_Assigned_Cases_AUM'', ''S3_Assigned_Cases_AUM'', ''S4_Assigned_Cases_AUM'', ''S1_Interest_Rate'', ''S2_Interest_Rate'', ''S3_Interest_Rate'', ''S4_Interest_Rate'', ''S1_No_of_Headcount'', ''S4_No_of_Headcount'', ''S2_No_of_Headcount'', ''S3_No_of_Headcount'') /*Allocation Out */ 
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'') 
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) IN (''PCM_ALLOCATION IN'', ''PCM_ALLOCATION OUT'')
						THEN (AMOUNT) 
						ELSE 0 
				END) AS ALLOCATED_PAT_FROM_HO,
				
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_PROFITLOSS'' 
							AND UPPER(TRIM(COST_CENTER)) NOT IN (''TOTAL_COST CENTER'', ''ALL_COST CENTER'')
							AND TRIM(RULE) IN (''S1_and_S2_SH_and_Above_No_of_Headcount_New_Business_YTD'', ''S1_09_Uttar_Pradesh'', ''S2_09_Uttar_Pradesh'', ''S1_SH_and_Above'', ''S2_SH_and_Above'', ''S3_SH_and_Above'', ''S4_SH_and_Above'', ''S2_and_S3_No_of_Headcount_New_Business_YTD_PE'',''Copy_to_New_Business_YTD_from_Headcount_Net_Balance_SH_and_Above'', /* Allocation In */ ''S1_09_Uttar_Pradesh'', ''S2_09_Uttar_Pradesh'', ''S1_SH_and_Above'', ''S2_SH_and_Above'', ''S3_SH_and_Above'', ''S4_SH_and_Above'', ''S1_SH_and_Above_No_of_Headcount'', ''S2_SH_and_Above_No_of_Headcount'') /* Allocation Out */ 
							AND UPPER(TRIM(VERTICAL)) NOT IN ( ''TOTAL_VERTICAL'' , ''ALL_VERTICAL'') 
							AND UPPER(TRIM(PRODUCT)) NOT IN ( ''TOTAL_PRODUCT'', ''ALL_PRODUCT'') 
							AND UPPER(TRIM(BALANCE)) IN (''PCM_ALLOCATION IN'', ''PCM_ALLOCATION OUT'')
						THEN (AMOUNT) 
						ELSE 0 
				END) AS ALLOCATED_PAT_FROM_BRANCH,
				
				
				-----------/* AFTER ALLOCATION */------------
				
				
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_40000000'' 
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER'' 
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_TOTAL_INCOME_AFTER_ALLOCATION,
					
					SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_41010000'' 
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_TOTAL_INTEREST_INCOME_AFTER_ALLOCATION,
					
					SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) IN (''AC_41030000'', ''AC_41040000'', ''AC_42000000'')
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_TOTAL_OTHER_INCOME_AFTER_ALLOCATION,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_50000000''
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
					END) -
                SUM(CASE 
                        WHEN UPPER(TRIM(ACCOUNT)) IN (''AC_51070000'', ''AC_51090000'')
                            AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
                            AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
                            AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
                            AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
                            AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
                            AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
                        THEN (AMOUNT)
                        ELSE 0
                        END) AS BRANCH_TOTAL_EXPENDITURE_AFTER_ALLOCATION,
						
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_51060000''
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_TOTAL_EMPLOYEE_BENEFIT_EXPENSE_AFTER_ALLOCATION,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) IN (''AC_51010000'', ''AC_51020000'', ''AC_51080000'')
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_TOTAL_OTHER_OPERATING_EXPENSES_AFTER_ALLOCATION,
					
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_51050000''
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
					END) AS BRANCH_TOTAL_PROVISION_FOR_DELIQUENCIES_AFTER_ALLOCATION,

				(BRANCH_TOTAL_INCOME_AFTER_ALLOCATION + BRANCH_TOTAL_EXPENDITURE_AFTER_ALLOCATION) AS BRANCH_TOTAL_PBT_AFTER_ALLOCATION,
					
					SUM(CASE 
                        WHEN UPPER(TRIM(ACCOUNT)) IN (''AC_51070000'', ''AC_51090000'')
                            AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
                            AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
                            AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
                            AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
                            AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
                            AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
                        THEN (AMOUNT)
                        ELSE 0 
                    END) AS BRANCH_TOTAL_DEPRECIATION_AND_TAX_EXPENSE_AFTER_ALLOCATION,
						
				SUM(CASE 
						WHEN UPPER(TRIM(ACCOUNT)) = ''AC_PROFITLOSS'' 
							AND UPPER(TRIM(COST_CENTER)) = ''TOTAL_COST CENTER''
							AND UPPER(TRIM(RULE)) = ''PCM_CALCULATION RULES''  
							AND UPPER(TRIM(VERTICAL)) = ''TOTAL_VERTICAL'' 
							AND UPPER(TRIM(PRODUCT)) = ''TOTAL_PRODUCT'' 
							AND UPPER(TRIM(BALANCE)) = ''PCM_NET BALANCE''
							AND UPPER(TRIM(SOURCE_POOL)) = ''TOTAL SOURCE POOL''
						THEN (AMOUNT) 
						ELSE 0 
				END) AS BRANCH_TOTAL_PAT_AFTER_ALLOCATION
				
				FROM
					(SELECT * FROM DISHA_L2_CURATED.BASE_MODEL_DATA.L2_BRANCH_PROFITABILITY_DETAILS WHERE UPPER(TRIM(BRANCH)) NOT IN (''TOTAL_BRANCH_LOCATION'',''TOTAL_BRANCH'')) BPD
				INNER JOIN
					(SELECT BRANCH FROM DISHA_L2_CURATED.BASE_HARDCODE.PCMCS_BRANCH WHERE UPPER(TRIM(DATA_STORAGE)) = ''STORE''
					AND UPPER(TRIM(PARENT)) NOT IN (''BRANCH'', ''BRANCH LOCATION'', ''TOTAL_BRANCH_LOCATION'', ''TOTAL_BRANCH'', ''BUSINESS STATES'')) PB
					ON BPD.BRANCH = PB.BRANCH
				INNER JOIN
					(SELECT * FROM DISHA_L2_CURATED.BASE_MODEL_DATA.BRANCH_DETAILS WHERE ACTIVATION_FLAG =1) BDSFDC
					ON SUBSTR(BPD.BRANCH,6) = CAST(BDSFDC.BRANCH_LMS_CODE AS VARCHAR)
					GROUP BY
				BPD.BRANCH,
				BDSFDC.BRANCH_LMS_CODE,
				BDSFDC.BRANCH_NAME,
				BDSFDC.BRANCH_STATE_NAME,
				BDSFDC.DATE_OF_OPENING,
				BPD.PERIOD || 
				CASE WHEN UPPER(BPD.PERIOD) IN (''JAN'', ''FEB'', ''MAR'') 
					THEN LPAD(CAST(SUBSTRING(BPD.YEAR, 3, 2) AS INT) + 1, 2, ''0'')
				ELSE 
					SUBSTRING(BPD.YEAR, 3, 2)
				END,
				LAST_DAY(TO_DATE(CONCAT(''01-'', MONTH_YEAR), ''DD-MonYY''))				
			)BID
			LEFT JOIN
			(SELECT  TO_CHAR(BUSINESS_DATE, ''MONYY'') AS RISK_REPORT_MONTH,BRANCH_ID,
				SUM(CASE WHEN UPPER(TRIM(PRODUCT_CATEGORY)) = ''HL''
					THEN (AUM * 10000000)
					ELSE 0
					END) AS HL_AUM,
			SUM(CASE WHEN UPPER(TRIM(PRODUCT_CATEGORY)) IN (''NHL'', ''5'')
					THEN (AUM * 10000000)
					ELSE 0
					END) AS NHL_AUM,
			(HL_AUM + NHL_AUM) AS NET_AUM
			FROM DISHA_MART_ANALYTICS.CREDIT_RISK_MART.RISK_LOAN_ACCOUNT_MART /*WHERE UPPER(TRIM(CURRENT_FLAG)) = ''TRUE'' */
			GROUP BY BRANCH_ID,TO_CHAR(BUSINESS_DATE, ''MONYY'')) RLM
			ON RLM.BRANCH_ID = BID.LMS_BRANCH_ID AND RLM.RISK_REPORT_MONTH=BID.MONTH_YEAR
			LEFT JOIN
			(SELECT BRANCH_LMS_CODE,TO_CHAR(SANCTION_DATE, ''MONYY'') AS SANC_REPORT_MTH, SUM(SANCTION_COUNT) AS NET_SANCTION_COUNT,SUM(SANCTION_AMOUNT)*10000000 AS NET_SANCTION_AMOUNT, SUM(SUD_COUNT) AS NET_SUD_COUNT, SUM(SUD_AMOUNT)*10000000 AS NET_SUD_AMOUNT FROM DISHA_MART_ANALYTICS.SALES_CREDIT_MART.LOAN_APPLICATION_MART WHERE CURRENT_FLAG = ''TRUE'' 
			GROUP BY BRANCH_LMS_CODE,TO_CHAR(SANCTION_DATE, ''MONYY'')
			) LAM_SANC
			ON LAM_SANC.BRANCH_LMS_CODE = BID.LMS_BRANCH_ID AND LAM_SANC.SANC_REPORT_MTH=BID.MONTH_YEAR
			LEFT JOIN
			(SELECT BRANCH_LMS_CODE,TO_CHAR(DEAL_FORWARD_DATE, ''MONYY'') AS DEAL_REPORT_MTH, SUM(LOGIN_COUNT) AS NET_LOGIN_COUNT,SUM(LOGIN_AMOUNT)*10000000 AS NET_LOGIN_AMOUNT FROM DISHA_MART_ANALYTICS.SALES_CREDIT_MART.LOAN_APPLICATION_MART WHERE CURRENT_FLAG = ''TRUE'' GROUP BY BRANCH_LMS_CODE,TO_CHAR(DEAL_FORWARD_DATE, ''MONYY'')
			) LAM_DEAL
			ON LAM_DEAL.BRANCH_LMS_CODE = BID.LMS_BRANCH_ID AND LAM_DEAL.DEAL_REPORT_MTH=BID.MONTH_YEAR
			LEFT JOIN
			(SELECT  TO_CHAR(LAST_DISBURSAL_DATE, ''MONYY'') AS DISB_REPORT_MONTH,BRANCH_ID,SUM(DISB_AMOUNT) AS NET_DISBURSAL_AMOUNT FROM DISHA_MART_ANALYTICS.SALES_CREDIT_MART.LOAN_ACCOUNT_MART WHERE UPPER(TRIM(CURRENT_FLAG)) = ''TRUE''
			GROUP BY BRANCH_ID, TO_CHAR(LAST_DISBURSAL_DATE, ''MONYY'')) LAM_DISB
			ON LAM_DISB.BRANCH_ID = BID.LMS_BRANCH_ID AND LAM_DISB.DISB_REPORT_MONTH=BID.MONTH_YEAR
            LEFT JOIN
			(SELECT TO_CHAR(SANCTION_DATE, ''MONYY'') AS SANC_MONTH,BRANCH_LMS_CODE,AVG(SANCTION_ROI) AS DISBURSAL_YIELD FROM DISHA_MART_ANALYTICS.SALES_CREDIT_MART.LOAN_APPLICATION_MART WHERE UPPER(TRIM(CURRENT_FLAG)) = ''TRUE''
			GROUP BY BRANCH_LMS_CODE,TO_CHAR(SANCTION_DATE, ''MONYY'')) DISB_YLD
			ON DISB_YLD.BRANCH_LMS_CODE = BID.LMS_BRANCH_ID AND DISB_YLD.SANC_MONTH=BID.MONTH_YEAR
			LEFT JOIN
			(SELECT SUM(PROCESSING_FEE) AS NET_PROCESSING_FEE, SUM(PDD_FEE) AS NET_PDD_FEE, SUM(ADMIN_FEE) AS NET_ADMIN_FEE, SUM(TOTAL_FEES_AMOUNT) AS NET_TOTAL_FEES_AMOUNT, SUM(PROPERTY_INSURANCE_FEE) AS NET_PROPERTY_INSURANCE_FEE, SUM(HEALTH_INSURANCE_FEE) AS NET_HEALTH_INSURANCE_FEE, SUM(LIFE_INSURANCE_FEE) AS NET_LIFE_INSURANCE_FEE, TO_CHAR(LOAN_INITIATION_DATE, ''MONYY'') AS FEE_REPORT_MONTH, BRANCH_ID
			FROM DISHA_MART_ANALYTICS.SALES_CREDIT_MART.LOAN_ACCOUNT_MART WHERE UPPER(TRIM(CURRENT_FLAG)) = ''TRUE''
			GROUP BY TO_CHAR(LOAN_INITIATION_DATE, ''MONYY''), BRANCH_ID) LACCM_FEE
			ON LACCM_FEE.BRANCH_ID = BID.LMS_BRANCH_ID AND LACCM_FEE.FEE_REPORT_MONTH = BID.MONTH_YEAR
			LEFT JOIN 
			(SELECT SUM(ST.DISBURSEMENT_TARGET_AMT) AS DISBURSAL_TARGET, BR.BRANCH_LMS_CODE, TO_CHAR(ST.MONTH_YEAR, ''MONYY'') AS DISB_TGT_MONTH
				FROM DISHA_L2_CURATED.BASE_MODEL_DATA.SALES_TARGET ST
			LEFT JOIN DISHA_L2_CURATED.HRMS.EMPLOYEE_MASTER EMP 
			ON ST.EMPLOYEE_ID = EMP.EMPLOYEE_ID
			LEFT JOIN (
				SELECT BRANCH_LMS_CODE 
				FROM DISHA_L2_CURATED.BASE_MODEL_DATA.BRANCH_DETAILS 
				WHERE CURRENT_FLAG = TRUE AND ACTIVATION_FLAG = 1
			) BR 
			ON TO_VARCHAR(BR.BRANCH_LMS_CODE) = SUBSTR(REPLACE(EMP.BRANCH, ''- '', ''-''), CHARINDEX(''-'', EMP.BRANCH) + 3)
			GROUP BY BR.BRANCH_LMS_CODE, TO_CHAR(ST.MONTH_YEAR, ''MONYY'')
			) DISB_TGT
			ON BID.LMS_BRANCH_ID = DISB_TGT.BRANCH_LMS_CODE AND BID.MONTH_YEAR = DISB_TGT.DISB_TGT_MONTH
			LEFT JOIN
			(SELECT BRANCH_LMS_CODE,
				SUM(CASE WHEN UPPER(TRIM(DEPARTMENT)) LIKE ''%SALES%'' AND UPPER(TRIM(EMP_LEVEL)) IN (''RELATIONSHIP OFFICER'',''REATIONSHIP OFFICER'',''APPRENTICE TRAINEE'',''INDIVIDUAL RELATIONSHIP MANAGER'',''MANAGEMENT TRAINEE'')
				THEN 1 
				ELSE 0
				END) AS SALES_RO_HEADCOUNT,
				SUM(CASE WHEN UPPER(TRIM(DEPARTMENT)) LIKE ''%SALES%'' AND UPPER(TRIM(EMP_LEVEL)) = ''RRO''
				THEN 1 
				ELSE 0 
				END) AS SALES_RRO_HEADCOUNT,
				SUM(CASE WHEN UPPER(TRIM(DEPARTMENT)) LIKE  ''%SALES%'' AND UPPER(TRIM(EMP_LEVEL)) =  ''BRANCH HEAD''
				THEN 1
				ELSE 0
				END) AS SALES_BH_HEADCOUNT,
				SUM(CASE WHEN UPPER(TRIM(DEPARTMENT)) LIKE ''%CREDIT%'' AND UPPER(TRIM(EMP_LEVEL)) =  ''CREDIT OFFICER''
				THEN 1
				ELSE 0
				END) AS CREDIT_CO_HEADCOUNT,
				SUM(CASE WHEN UPPER(TRIM(DEPARTMENT)) LIKE  ''%CREDIT%'' AND UPPER(TRIM(EMP_LEVEL)) IN (''CREDIT MANAGER'', ''ASSISTANT MANAGER-CREDIT'', ''DEPUTY MANAGER-CREDIT'', ''DEPUTY MANAGER'', ''MANAGEMENT TRAINEE'')
				THEN 1
				ELSE 0
				END) AS CREDIT_CM_HEADCOUNT
				FROM 
				(SELECT * FROM DISHA_L2_CURATED.HRMS.EMPLOYEE_MASTER WHERE ACTIVATION_FLAG = 1 AND DATEOFRELIEVING IS NULL OR DATEOFRELIEVING>CURRENT_DATE) E 
			LEFT JOIN 
				(SELECT * FROM DISHA_L2_CURATED.BASE_MODEL_DATA.BRANCH_DETAILS WHERE CURRENT_FLAG= TRUE AND ACTIVATION_FLAG = 1) BR
				ON TO_VARCHAR(BR.BRANCH_LMS_CODE)=SUBSTR(REPLACE(E.BRANCH, ''- '',''-''), CHARINDEX(''-'',E.BRANCH,0)+3,1000)
				GROUP BY BRANCH_LMS_CODE) AS EMP_CNT
				ON EMP_CNT.BRANCH_LMS_CODE =  BID.LMS_BRANCH_ID
			QUALIFY ROW_NUMBER() OVER (PARTITION BY BID.LMS_BRANCH_ID,BID.MONTH_YEAR ORDER BY BID.MONTH_YEAR)=1 
	);

